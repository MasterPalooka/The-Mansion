Password Lock Code:
local side = "left"
local password = "123"
local opentime = 4
while true do
  term.clear()
  term.setCursorPos(1,1)
  write("Password: ")
  local input = read("#")
  if input == password then
    term.clear()
    term.setCursorPos(1,1)
    print("Buenos Dias")
    rs.setOutput(side,true)
    sleep(opentime)
    rs.setOutput(side,false)
  else  
    print("No Bueno")
    sleep(2)
  end
end
